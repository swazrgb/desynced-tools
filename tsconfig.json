{
    "compilerOptions":{
        "lib":[
            "ES6"
        ],
        "strict":true,
        "target":"ES2020",
        "moduleResolution": "nodenext",
        "module": "nodenext",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "noImplicitAny": false,
        "allowJs": true,
        "sourceMap": true,
        "inlineSourceMap": true,
        // "checkJs": true,
        "outDir": "dist",
        },
        "include": [
            "decompile/*.ts",
            "*.ts",
            "*.js",
            "package.json"
        ],
        "exclude": ["compiler_bundle.ts"]
}
